<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.drw_eng.eng_reviewer">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.drw_eng.eng_reviewer.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.drw_eng.eng_reviewer.Fragment.EnrollFragment"
            tools:ignore="Instantiatable"></activity>
        <activity android:name="com.drw_eng.eng_reviewer.etc.NewsActivity"></activity>
        <activity android:name="com.drw_eng.eng_reviewer.Fragment.ReviewerFragment"
            tools:ignore="Instantiatable"></activity>
        <activity android:name="com.drw_eng.eng_reviewer.MainActivity">
        </activity>
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-1195074851560755~1857757096">
        </meta-data>
    </application>

</manifest>